<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gxa.mapper.AdminMapper">
    <insert id="insert" parameterType="admin">
       INSERT INTO t_admin(name,sex,age,number,address,position,status)
        VALUES (#{name},#{sex},#{age},#{number},#{address},#{position},#{status})
    </insert>
    <update id="update" >
        UPDATE t_admin
        SET status=0
        WHERE id=#{id}
    </update>
    <select id="queryAll" resultType="admin">
        SELECT *
        FROM t_admin WHERE status=1
    </select>
</mapper>